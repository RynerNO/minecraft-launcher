{
  "name": "minecraft-launcher",
  "version": "2.1.0",
  "scripts": {
    "app:watch": "tsc -w -p ./src/app/tsconfig.json",
    "view:watch": "webpack serve",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "concurrently \"yarn app:watch\" \"yarn view:watch\" \"nodemon --exec electron ./dist/app/main.js\"",
    "build": "webpack build && tsc -p ./src/app/tsconfig.json && electron-builder --win --x64"
  },
  "repository": "https://github.com/RynerNO/minecraft-launcher.git",
  "author": "RynerNO",
  "description": "Download, update and launch minecraft",
  "license": "MIT",
  "main": "./dist/app/main.js",
  "build": {
    "artifactName": "${productName}-${version}.${ext}",
    "extraResources": [
      "**/build/7za.exe"
    ],
    "buildVersion": "2.1.0",
    "directories": {
      "output": "package"
    },
    "appId": "6aba67f12756774f123ade16ade29056",
    "files": [
      "**/dist/**/*",
      "**/dist/app/main.js"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.37",
    "@types/node-7z": "^2.1.0",
    "@types/unzipper": "^0.10.3",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.1-alpha.12",
    "@typescript-eslint/parser": "^4.19.1-alpha.12",
    "@vue/compiler-sfc": "^3.0.9",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "css-loader": "^5.2.0",
    "electron": "^13.0.0-beta.8",
    "electron-builder": "^22.10.5",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "folder-hash": "^4.0.1",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primevue": "^3.3.5",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass-resources-loader": "^2.1.1",
    "secure-ls": "^1.2.6",
    "style-loader": "^2.0.0",
    "sugarss": "^3.0.3",
    "ts-loader": "^8.1.0",
    "typescript": "^4.3.0-dev.20210329",
    "validator": "^13.5.2",
    "vue": "^3.0.11",
    "vue-loader": "^16.2.0",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.0.0-beta.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0-beta.1",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "electron-updater": "^4.3.8",
    "mcproto": "^0.9.0",
    "minecraft-launcher-core": "RynerNO/MinecraftLauncher-core",
    "node-7z": "^2.1.2",
    "open": "^8.0.4"
  }
}
